package org.tamina.babylon.tools.math;
class Rectangle {
    var height : Float;
    var width : Float;
    var x : Float;
    var y : Float;

    public function new(x : Float = 0, y : Float = 0, width : Float = 0, height : Float = 0) {
        this.x = x;
        this.y = y;
        this.width = width;
        this.height = height;
    }
}
